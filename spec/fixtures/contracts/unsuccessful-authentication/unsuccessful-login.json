{
   "request":{
      "method":"POST",
      "path":"/v2.0/tokens",
      "headers":{
         "Accept":"application/json",
         "Encoding":"UTF-8"
      },
      "body":{
         "auth":{
            "passwordCredentials":{
               "username":"<rackspace-username>",
               "password":"<rackspace-username>"
            }
         }
      },
      "params":{

      }
   },
   "response":{
      "status":401,
      "headers":{
         "Content-Type":"application/json"
      },
      "body":{
         "type":"object",
         "$schema":"http://json-schema.org/draft-03/schema",
         "required":true,
         "properties":{
            "unauthorized":{
               "type":"object",
               "required":true,
               "properties":{
                  "code":{
                     "type":"number",
                     "default":401,
                     "required":true
                  },
                  "message":{
                     "type":"string",
                     "default":"Unable to authenticate user with credentials provided.",
                     "required":true
                  }
               }
            }
         }
      }
   }
}